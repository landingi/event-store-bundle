{
    "name": "landingi/event-store-bundle",
    "license": "MIT",
    "description": "Landingi event store bundle implementation",
    "type": "symfony-bundle",
    "autoload" : {
        "psr-4" : {
            "Landingi\\EventStoreBundle\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Landingi\\EventStoreBundle\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "doctrine/dbal": "^2.0|^3.0",
        "symfony/uid": "^5.2",
        "symfony/http-client": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard": "^9.0",
        "phpstan/phpstan": "^0.12"
    }
}
