{
    "name": "landingi/event-store-bundle",
    "license": "MIT",
    "description": "Landingi event store bundle implementation",
    "type": "symfony-bundle",
    "autoload" : {
        "psr-4" : {
            "Landingi\\EventStoreBundle\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Landingi\\EventStoreBundle\\" : "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/framework-bundle": "6.0.*",
        "doctrine/dbal": "^2.0|^3.0",
        "symfony/uid": "6.0.*",
        "symfony/http-client": "6.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "landingi/php-coding-standards": "^1.0.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
